            </ruleml:Exp>
            <ruleml:Ind iri="buvo:nz.lit.wetting"/>
        </ruleml:Atom>
      </ruleml:And>
    </ruleml:if>
    <ruleml:then>
      <lrml:Obligation iri="devo:Achievement">
        <ruleml:GreaterEqual>
          <ruleml:Exp>
            <ruleml:Fun iri="buvo:nz.lit.CrossFallOf"/>
