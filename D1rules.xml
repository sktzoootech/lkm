        <lrml:DefeasibleStrength iri="http://spin.nicta.com.au/spindle/ruleStrength#defeasible"/>
    </lrml:hasStrength>
    <ruleml:if>
      <ruleml:And>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.accessRoute" />
          <ruleml:Var key="access_route">X</ruleml:Var>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.accessibleRoute" />
          <ruleml:Var key="accessible_route">Y</ruleml:Var>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:BeingSubjectTo">
            <ruleml:Exp>
              <ruleml:Or>
                <ruleml:Atom>
                  <ruleml:Fun iri="buvo:nz.lit.SurfaceOf"/>
                  <ruleml:Var keyref="#access_route" />
                </ruleml:Atom>
                <ruleml:Atom>
