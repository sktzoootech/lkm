    <lrml:hasStrength>
        <lrml:DefeasibleStrength iri="http://spin.nicta.com.au/spindle/ruleStrength#defeasible"/>
    </lrml:hasStrength>
    <ruleml:if>
      <ruleml:And>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.accessibleBuilding" />
          <ruleml:Var key="accessible_building">X</ruleml:Var>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.accessibleStairway" />
          <ruleml:Var key="accessible_stairway">Y</ruleml:Var>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.accessibleRoute" />
          <ruleml:Var key="accessible_route">Z</ruleml:Var>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.leadingOff"/>
            <ruleml:Var keyref="#accessible_stairway" />
            <ruleml:Var keyref="#accessible_route" />
        </ruleml:Atom>
      </ruleml:And>
    </ruleml:if>
    <ruleml:then>
      <ruleml:Obligation iri="devo:Achievement">
        <ruleml:Rel iri="buvo:nz.lit.Have"/>
          <ruleml:Var keyref="#accessible_building" />
          <ruleml:Var keyref="#accessible_stairway" />
      </ruleml:Obligation>
  </ruleml:Rule>
</lrml:PrescriptiveStatement>


