<lrml:PrescriptiveStatement key="D1.4.3.1.exception">
  <ruleml:Rule key="D1.4.3.1.exception">
    <lrml:hasStrength>
        <lrml:DefeasibleStrength iri="http://spin.nicta.com.au/spindle/ruleStrength#defeasible"/>
    </lrml:hasStrength>
    <ruleml:if>
      <ruleml:And>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.landing" />
          <ruleml:Var key="landing">X</ruleml:Var>
        </ruleml:Atom>
        <ruleml:Atom>
          <ruleml:Rel iri="buvo:nz.lit.stairs" />
          <ruleml:Var key="stairs">Y</ruleml:Var>
        </ruleml:Atom>
        <ruleml:LessEqual>
          <ruleml:Exp>
            <rulem:Fun iri="buvo:nz.lit.RiseOf" />
            <ruleml:Exp>
              <rulem:Fun iri="buvo:nz.lit.FlightOf" />
              <ruleml:Var keyref="#stairs" />
          </ruleml:Exp>
        </ruleml:Exp>
        <ruleml:Ind>600 mm</ruleml:Ind>
        </ruleml:LessEqual>
        <ruleml:Or>
          <ruleml:Atom>
            <ruleml:Rel iri="buvo:nz.lit.SlidesAwayFrom" />
            <ruleml:Exp>
              <rulem:Fun iri="buvo:nz.lit.DoorOpensInto" />
              <ruleml:Var keyref="#stairs" />
            </ruleml:Exp>
            <ruleml:Exp>
              <rulem:Fun iri="buvo:nz.lit.StepsOf" />
              <ruleml:Var keyref="#stairs" />
            </ruleml:Exp>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="buvo:nz.lit.OpensAwayFrom" />
            <ruleml:Exp>
              <rulem:Fun iri="buvo:nz.lit.DoorOpensInto" />
              <ruleml:Var keyref="#stairs" />
            </ruleml:Exp>
            <ruleml:Exp>
              <rulem:Fun iri="buvo:nz.lit.StepsOf" />
              <ruleml:Var keyref="#stairs" />
            </ruleml:Exp>
          </ruleml:Atom>
        </ruleml:Or>
      </ruleml:And>
    </ruleml:if>
    <ruleml:then>
      <lrml:Permission>
        <ruleml:Neg>
          <ruleml:Atom>
            <ruleml:Rel iri="buvo:nz.lit.ProvidedBetween" />
              <ruleml:Var keyref="#landing">
              <ruleml:Exp>
                <ruleml:Fun iri="buvo:nz.lit.FlightOf" />
                  <ruleml:Var keyref="#stairway">
              </ruleml:Exp>
              <ruleml:Exp>
                <ruleml:Fun iri="buvo:nz.lit.DoorOpensInto" />
                  <ruleml:Var keyref="#stairway">
              </ruleml:Exp>
          </ruleml:Atom>
        </ruleml:Neg>
      </lrml:Permission>
    </ruleml:then>
  </ruleml:Rule>
</lrml:PrescriptiveStatement>

