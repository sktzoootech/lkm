      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Or>
            <ruleml:Atom>
              <ruleml:Rel iri="0105">
                Mechanical supply with mechanical extraction,
              </ruleml:Rel>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Rel iri="0106">
                Mechanical supply with natural exhaust.
              </ruleml:Rel>
            </ruleml:Atom>
          </ruleml:Or>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.2.2"/>
    <lrml:toTarget keyref="#2.2.2.a"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.2.2.a">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:And>
          <ruleml:Atom>
            <ruleml:Rel iri="0011">
              A mechanical ventilation system
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0138">
              for forced or induced draught appliances, and
            </ruleml:Rel>
          </ruleml:Atom>
        </ruleml:And>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Atom>
            <ruleml:Fun iri="buvo:Rate">
              <ruleml:Rel iri="0107">
                For each kW of gas consumption (of all appliances in the plant room) provide outdoor air at the rate of:
      						i) 3.6 m3/h
              </ruleml:Rel>
              <ruleml:Ind>3.6 m3/h per kW of gas</ruleml:Ind>
            </ruleml:Fun>
          </ruleml:Atom>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.2.2"/>
    <lrml:toTarget keyref="#2.2.2.b"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.2.2.b">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:And>
          <ruleml:Atom>
            <ruleml:Rel iri="0011">
              A mechanical ventilation system
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0139">
              for appliances with atmospheric burners,
            </ruleml:Rel>
          </ruleml:Atom>
        </ruleml:And>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Atom>
            <ruleml:Fun iri="buvo:Rate">
              <ruleml:Rel iri="0107">
                For each kW of gas consumption (of all appliances in the plant room) provide outdoor air at the rate of:
                  ii) 7.2 m3/h
              </ruleml:Rel>
              <ruleml:Ind>7.2 m3/h per kW of gas</ruleml:Ind>
            </ruleml:Fun>
          </ruleml:Atom>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.2.2"/>
    <lrml:toTarget keyref="#2.2.2.c"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.2.2.c">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Rel iri="0011">
            A mechanical ventilation system
          </ruleml:Rel>
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Or>
            <ruleml:Atom>
              <ruleml:Fun iri="buvo:Rate">
                <ruleml:Rel iri="0108">
                  Remove exhaust air from the room either:
        						i) mechanically at one third the inlet rate, or
                </ruleml:Rel>
                <ruleml:Ind>1/3 of the inlet</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Fun iri="buvo:Rate">
                <ruleml:Rel iri="0141">
                  Remove exhaust air from the room either:
        						ii) naturally via high-level openings having a free ventilation area of no less than 600 mm2 per kW of total gas consumption for all appliances in the room.
                </ruleml:Rel>
                <ruleml:Ind>600 mm2 per kW of total gas consumption</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
          </ruleml:Or>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.3.1"/>
    <lrml:toTarget keyref="#2.3.1.a"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.3.1.a">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Rel iri="0109">
            A flue system
          </ruleml:Rel>
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Or>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherEqualTo">
                <ruleml:Rel iri="0110">
                  The cross-sectional area of a natural draught flue system external to the appliances,
                </ruleml:Rel>
                <ruleml:Rel iri="0075">
                  no less than the cross-sectional area of the appliance outlet,
                </ruleml:Rel>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Rel iri="0111">
                The flue designed to comply with AS/NZS 5601.1, section 6.7 and Appendix H, and (Amend 3 Feb 2014)
              </ruleml:Rel>
            </ruleml:Atom>
          </ruleml:Or>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.3.1"/>
    <lrml:toTarget keyref="#2.3.1.b"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.3.1.b">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:And>
          <ruleml:Atom>
            <ruleml:Rel iri="0109">
              A flue system
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0112">
              If a draught diverter is not fitted:
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0142">
              unless the discharge at other locations can be achieved without hazard to persons, property or appliance operation,
            </ruleml:Rel>
          </ruleml:Atom>
        </ruleml:And>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Atom>
            <ruleml:Rel iri="0143">
    					i) flue products discharged to the atmosphere only at the flue terminal,
            </ruleml:Rel>
          </ruleml:Atom>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.3.1"/>
    <lrml:toTarget keyref="#2.3.1.c"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.3.1.c">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:And>
          <ruleml:Atom>
            <ruleml:Rel iri="0109">
              A flue system
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0112">
              If a draught diverter is not fitted:
            </ruleml:Rel>
          </ruleml:Atom>
        </ruleml:And>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Atom>
            <ruleml:Rel iri="0144">
  						ii) a method of automatically shutting down the main burners of forced or induced draught appliances, should the normal free discharge of the flue be interrupted.
            </ruleml:Rel>
          </ruleml:Atom>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.3.2"/>
    <lrml:toTarget keyref="#2.3.2"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.3.2">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:And>
          <ruleml:Atom>
            <ruleml:Rel iri="0113">
              Draught diverter installations shall discharge the total
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0109">
              flue products
            </ruleml:Rel>
          </ruleml:Atom>
        </ruleml:And>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:Atom>
            <ruleml:Rel iri="0114">
              including excess air and draught diverter dilution air, at the flue terminal without spillage from the skirt of the draught diverter.
            </ruleml:Rel>
          </ruleml:Atom>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="2.4.1"/>
    <lrml:toTarget keyref="#2.4.1"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#2.4.1">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:And>
          <ruleml:Atom>
            <ruleml:Rel iri="0109">
              The location of a flue terminal
            </ruleml:Rel>
          </ruleml:Atom>
          <ruleml:Atom>
            <ruleml:Rel iri="0137">
              on a dwelling
            </ruleml:Rel>
          </ruleml:Atom>
        </ruleml:And>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:And>
            <ruleml:Atom>
              <ruleml:Rel iri="0115">
                Outlets from natural draught flues or chimneys, positioned relative to surrounding construction to avoid wind causing down draughts in the flue,
              </ruleml:Rel>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherEqualTo">
                <ruleml:Rel iri="0116">
                  Flue pipes which extend through the roof, terminated no closer than:
        						i) 500 mm to the nearest part of any roof,
                </ruleml:Rel>
                <ruleml:Ind>500 mm to the nearest part of any roof,</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherEqualTo">
                <ruleml:Rel iri="0145">
                  Flue pipes which extend through the roof, terminated no closer than:
        						ii) 2.0 m to the roof level of a flat roof intended for personal or public use, and
                </ruleml:Rel>
                <ruleml:Ind>2.0 m to the roof level of a flat roof intended for personal or public use, and</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherEqualTo">
                <ruleml:Rel iri="0146">
                  Flue pipes which extend through the roof, terminated no closer than:
        						iii) 500 mm above any parapet,
                </ruleml:Rel>
                <ruleml:Ind>iii) 500 mm above any parapet,</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Rel iri="0117">
                Flues which terminate on the wall of a building located clear of inlets for outside air in accordance with the minimum clearances specified in AS/NZS 5601.1, section 6.9 and Figure 6.2.
              </ruleml:Rel>
            </ruleml:Atom>
          </ruleml:And>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

  <lrml:Association>
    <lrml:appliesSource keyref="3.0"/>
    <lrml:toTarget keyref="#3.0"/>
  </lrml:Association>

  <scope keyref="#3.0">
    Another Solution for Gas-fuel Appliances
  </scope>

  <lrml:Association>
    <lrml:appliesSource keyref="3.0.1"/>
    <lrml:toTarget keyref="#3.0.1"/>
  </lrml:Association>

  <lrml:PrescriptiveStatement key="#3.0.1">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Rel iri="0082">
            IMPLICIT (Gas-fuel Appliances)
          </ruleml:Rel>
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <lrml:Permission iri="devo:Permission">
          <ruleml:Atom>
            <ruleml:Rel iri="0119">
              AS/NZS 5601.1 Sections 1, 3, 4, 5 and 6 and Appendices A – M and O - R is an Acceptable Solution, but may exceed the performance criteria of NZBC G4.
            </ruleml:Rel>
          </ruleml:Atom>
        </lrml:Permission>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

</lrml:LegalRuleML>
