  <lrml:PrescriptiveStatement key="#1.3.8">
   <ruleml:Rule>
      <ruleml:if>
        <ruleml:Atom>
          <ruleml:Rel iri="0043">
            The terminal of a passive stack ventilator
          </ruleml:Rel>
        </ruleml:Atom>
      </ruleml:if>
      <ruleml:then>
        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:And>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherThan">
                <ruleml:Fun iri="buvo:Of">
                  <ruleml:Rel iri="0197">
                    have an equivalent aerodynamic area
                  </ruleml:Rel>
                  <ruleml:Rel iri="0043">
                    The terminal of a passive stack ventilator
                  </ruleml:Rel>
                </ruleml:Fun>
                <ruleml:Fun iri="buvo:Of">
                  <ruleml:Rel iri="0197">
                    have an equivalent aerodynamic area
                  </ruleml:Rel>
                  <ruleml:Rel iri="0044">
                    greater than the cross-sectional area of the stack,
                  </ruleml:Rel>
                </ruleml:Fun>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Rel iri="0045">
                extend above the roof to at least the ridge height.
              </ruleml:Rel>
            </ruleml:Atom>
          </ruleml:And>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
