        <lrml:Obligation iri="devo:Maintenance">
          <ruleml:And>
            <ruleml:Atom>
              <ruleml:Rel iri="0035">
                be designed in accordance with AS/NZS 4740 Section 3,
              </ruleml:Rel>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Rel iri="0036">
                be designed to achieve extract airflow rates specified in AS 1668.2 Table B1, using the following parameters:
                <ruleml:Fun iri="lovo:List">
                  <ruleml:Fun iri="lovo:Parameter">
                    <ruleml:Rel iri="0191">
                      Air Density r =
                    </ruleml:Rel>
                    <ruleml:Ind>1.2kg/m3</ruleml:Ind>
                  </ruleml:Fun>
                  <ruleml:Fun iri="lovo:Parameter">
                    <ruleml:Rel iri="0192">
                      Gravitational Constant g =
                    </ruleml:Rel>
                    <ruleml:Ind>9.81 m/s2</ruleml:Ind>
                  </ruleml:Fun>
                  <ruleml:Fun iri="lovo:Parameter">
                    <ruleml:Rel iri="0193">
                      Temperature Differential DT =
                    </ruleml:Rel>
                    <ruleml:Ind>3K</ruleml:Ind>
                  </ruleml:Fun>
                  <ruleml:Fun iri="lovo:Parameter">
                    <ruleml:Rel iri="0194">
                      Outside Ambient T =
                    </ruleml:Rel>
                    <ruleml:Ind>300K</ruleml:Ind>
                  </ruleml:Fun>
                </ruleml:Fun>
              </ruleml:Rel>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Fun iri="buvo:Integrate">
                <ruleml:Rel iri="0025">
                  Passive stack ventilators
                </ruleml:Rel>
                <ruleml:Rel iri="0001">
                  be integrated into the building
                </ruleml:Rel>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Negation>
              <ruleml:Atom>
                <ruleml:Rel iri="0037">
                  without decreasing the performance of the building envelope and
                </ruleml:Rel>
              </ruleml:Atom>
            </ruleml:Negation>
            <ruleml:Negation>
              <ruleml:Atom>
                <ruleml:Rel iri="0195">
                  decreasing the performance the partition walls of the building for external moisture, fire and acoustics,
                </ruleml:Rel>
              </ruleml:Atom>
            </ruleml:Negation>
            <ruleml:Or>
              <ruleml:Atom>
                <ruleml:Fun iri="buvo:Through">
                  <ruleml:Rel iri="0038">
                    be capable of drawing air
                  </ruleml:Rel>
                  <ruleml:Rel iri="0024">
                    through trickle ventilators
                  </ruleml:Rel>
                </ruleml:Fun>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Fun iri="buvo:Through">
                  <ruleml:Rel iri="0038">
                    be capable of drawing air
                  </ruleml:Rel>
                  <ruleml:Rel iri="0196">
                    permanent openings from the room or adjacent spaces.
                  </ruleml:Rel>
                </ruleml:Fun>
              </ruleml:Atom>
            </ruleml:Or>
            <ruleml:Or>
              <ruleml:Atom>
                <ruleml:Fun iri="lovo:GreatherThan">
                  <ruleml:Fun iri="buvo:Of">
                    <ruleml:Rel iri="0197">
                      aerodynamic area greater than
                    </ruleml:Rel>
                    <ruleml:Rel iri="0196">
                      The permanent openings to the surrounding spaces
                    </ruleml:Rel>
                  </ruleml:Fun>
                  <ruleml:Fun iri="buvo:Of">
                    <ruleml:Rel iri="0197">
                      equivalent aerodynamic area of the
                    </ruleml:Rel>
                    <ruleml:Rel iri="0025">
                      passive stack ventilator
                    </ruleml:Rel>
                  </ruleml:Fun>
                </ruleml:Fun>
              </ruleml:Atom>
              <ruleml:Atom>
                <ruleml:Fun iri="lovo:GreatherThan">
                  <ruleml:Fun iri="buvo:Of">
                    <ruleml:Rel iri="0197">
                      aerodynamic area greater than
                    </ruleml:Rel>
                    <ruleml:Rel iri="0024">
                      trickle ventilators to the outside
                    </ruleml:Rel>
                  </ruleml:Fun>
                  <ruleml:Fun iri="buvo:Of">
                    <ruleml:Rel iri="0197">
                      equivalent aerodynamic area of the
                    </ruleml:Rel>
                    <ruleml:Rel iri="0025">
                      passive stack ventilator
                    </ruleml:Rel>
                  </ruleml:Fun>
                </ruleml:Fun>
              </ruleml:Atom>
            </ruleml:Or>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherEqualTo">
                <ruleml:Rel iri="0041">
                  have ventilation ducts and stacks that are insulated in any unheated areas with a minimum thickness of 25 mm
                </ruleml:Rel>
                <ruleml:Ind>25 mm</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Fun iri="lovo:GreatherEqualTo">
                <ruleml:Rel iri="0198">
                  have ventilation ducts and stacks that are insulated in any unheated areas of a material having a thermal conductivity of no less than 0.04W/m2K
                </ruleml:Rel>
                <ruleml:Ind>0.04W/m2K</ruleml:Ind>
              </ruleml:Fun>
            </ruleml:Atom>
            <ruleml:Atom>
              <ruleml:Rel iri="0042">
                have a condensation trap fitted to the part of the duct above the roof level.
              </ruleml:Rel>
            </ruleml:Atom>
          </ruleml:And>
        </lrml:Obligation>
      </ruleml:then>
    </ruleml:Rule>
  </lrml:PrescriptiveStatement>

